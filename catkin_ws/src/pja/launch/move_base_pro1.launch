<launch>
  <!-- Arguments -->
<!--  <arg name="model" default="$(env TURTLEBOT3_MODEL)" doc="model type [burger, waffle, waffle_pi]"/>-->
<!--  <arg name="cmd_vel_topic" default="move_base/cmd_vel" />-->
  <arg name="cmd_vel_topic" default="/cmd_vel" />
  <arg name="odom_topic" default="wheel_odom" />
  <arg name="move_forward_only" default="false"/>

  <!-- move_base -->
  <node pkg="move_base" type="move_base" respawn="false" name="move_base" output="screen">
    <!-- <param name="base_local_planner" value="dwa_local_planner/DWAPlannerROS" /> -->
    <!-- <param name="base_local_planner" value="teb_local_planner/TebLocalPlannerROS" /> -->
    <param name="base_global_planner" value="global_planner/GlobalPlanner" />
    <rosparam file="$(find pja)/params/move_base_pro1.yaml" command="load" />
    <rosparam file="$(find pja)/params/move_base_pro1.yaml" command="load" ns="global_costmap" />
    <rosparam file="$(find pja)/params/move_base_pro1.yaml" command="load" ns="local_costmap" />
    <remap from="cmd_vel" to="$(arg cmd_vel_topic)"/>
    <remap from="odom" to="$(arg odom_topic)"/>
    <!-- <remap from="map" to="map_new"/> -->
    <!-- <remap from="os_cloud_node/points" to="os_cloud_node/points_filter"/> -->
    <param name="DWAPlannerROS/min_vel_x" value="0.0" if="$(arg move_forward_only)" />
  </node>
</launch>
