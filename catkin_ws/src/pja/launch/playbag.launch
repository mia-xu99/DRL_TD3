<launch>


  <param name="use_sim_time" value="true"/>

  <include  file="$(find ouster_ros)/ouster.launch" >
  <arg name="replay" default="true"/>
  </include>




 <node name="cartographer_node" pkg="cartographer_ros"
      type="cartographer_node" args="
          -configuration_directory $(find cartographer_ros)/configuration_files
          -configuration_basename my3d.lua"
      output="screen">
    <remap from="points2" to="/os_cloud_node/points_out" />
    <remap from="imu" to="/os_cloud_node/imu" />
    <remap from="odom" to="/wheel_odom" />
  </node>

  <node name="cartographer_occupancy_grid_node" pkg="cartographer_ros"
      type="cartographer_occupancy_grid_node" args="-resolution 0.05 -publish_period_sec 1" >
    <remap from="map" to="map_new"/>
      </node> 



<!-- <include file="$(find aloam_velodyne)/launch/aloam_velodyne_VLP_16.launch"  /> -->

<!-- <include file="$(find pja)/launch/move_base_pro1.launch"  /> -->

<node pkg="rviz" type="rviz" name="rviz_my" 
          args="-d $(find turtlebot3_slam)/rviz/turtlebot3_cartographer.rviz"/>
          
    <node pkg="rqt_gui" type="rqt_gui" name="rqt" required="true"/>


</launch>
