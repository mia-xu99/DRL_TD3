<launch>
<arg name="gui" value="true"/>

   <!-- Gazebo  -->
  <include file="$(find multi_robot_scenario)/launch/empty_world.launch">
    <arg name="gui" value="$(arg gui)"/>
  </include>

  <!-- Robot with Autolabor Pro1 and Ouster LiDAR -->
  <include file="$(find multi_robot_scenario)/launch/autolabor_pro1.gazebo.launch">
     <arg name="robot_name" value="r1" />
     <arg name="robot_position" value="-x 0.0 -y 0.0 -z 0.01 -R 0 -P 0 -Y 0.0" />
   </include>

  <!-- RVIZ -->
  <node pkg="rviz" type="rviz" name="rviz" args="-d $(find multi_robot_scenario)/launch/autolabor_pro1.rviz" />

</launch>
